	<!-- Mapbox -->
	<link href="https://api.mapbox.com/mapbox-gl-js/v2.5.1/mapbox-gl.css" rel="stylesheet">
	<script src="https://api.mapbox.com/mapbox-gl-js/v2.5.1/mapbox-gl.js"></script>
	
	<!-- Mapbox -->
	<script>
    mapboxgl.accessToken = 'pk.eyJ1IjoiYXJwYW5ldGl0YWxpYSIsImEiOiJja3ZiZGxmbTEya3hmMnBsdWVhZWFuNmw1In0.vMR0-fmLRsFHd2OZyLrrrw';
    const map = new mapboxgl.Map({
     container: 'map',
     style: 'mapbox://styles/mapbox/streets-v11',
	 center: [9.1009068, 45.4783899],
     zoom: 15
	 
    });
	
	//const popup = new mapboxgl.Popup()
	//.setLngLat([9.1009068, 45.4783899])
	//.setHTML('<p><a href="https://bit.ly/3mPB8fU" target="_blank">via Caldera 21, Milano</a></p>')
	//.addTo(map);
	
	map.on('load', () => {
	// Add an image to use as a custom marker
	map.loadImage(
	'https://docs.mapbox.com/mapbox-gl-js/assets/custom_marker.png',
	(error, image) => {
	if (error) throw error;
	map.addImage('custom-marker', image);
	// Add a GeoJSON source with 2 points
	map.addSource('points', {
	'type': 'geojson',
	'data': {
	'type': 'FeatureCollection',
	'features': [
	{
	
	'type': 'Feature',
	'geometry': {
	'type': 'Point',
	'coordinates': [
	9.1009068, 45.4783899
	]
	},
	'properties': {
	'title': 'via Caldera 21, Milano'
	}
	}

	]
	}
	});
	 
	// Add a symbol layer
	map.addLayer({
	'id': 'points',
	'type': 'symbol',
	'source': 'points',
	'layout': {
	'icon-image': 'custom-marker',
	// get the title name from the source's "title" property
	'text-field': ['get', 'title'],
	'text-font': [
	'Open Sans Semibold',
	'Arial Unicode MS Bold'
	],
	'text-offset': [0, 1.25],
	'text-anchor': 'top'
	}
	});
	}
	);
	});
	
	
   </script>
   

